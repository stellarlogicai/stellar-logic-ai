{
  "name": "Financial AI Security Plugin",
  "version": "1.0.0",
  "enabled": true,
  "settings": {
    "max_concurrent_requests": 200,
    "timeout": 45,
    "retry_attempts": 3,
    "log_level": "INFO"
  },
  "security": {
    "pci_dss_compliance": true,
    "data_encryption": true,
    "fraud_detection": true,
    "audit_logging": true
  },
  "api": {
    "rate_limit": 2000,
    "authentication_required": true,
    "cors_enabled": true
  }
}