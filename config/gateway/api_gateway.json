{
  "api_gateway": {
    "name": "Stellar Logic AI API Gateway",
    "version": "1.0.0",
    "enabled": true,
    "settings": {
      "host": "0.0.0.0",
      "port": 8080,
      "workers": 4,
      "timeout": 30,
      "log_level": "INFO"
    },
    "security": {
      "authentication": {
        "enabled": true,
        "type": "OAuth 2.0",
        "token_endpoint": "/oauth/token"
      },
      "rate_limiting": {
        "enabled": true,
        "default_limit": 1000,
        "burst_limit": 100
      },
      "cors": {
        "enabled": true,
        "allowed_origins": [
          "*"
        ],
        "allowed_methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "OPTIONS"
        ]
      }
    },
    "plugins": {
      "auto_discovery": true,
      "health_check_interval": 30,
      "connection_timeout": 10
    }
  }
}