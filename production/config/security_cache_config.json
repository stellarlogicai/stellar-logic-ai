{
  "security_cache": {
    "enabled": true,
    "default_ttl": 300,
    "max_memory_size": 104857600,
    "cleanup_interval": 60,
    "cache_stores": {
      "memory": {
        "enabled": true,
        "max_size": 1000
      },
      "redis": {
        "enabled": false,
        "host": "localhost",
        "port": 6379
      },
      "file": {
        "enabled": true,
        "directory": "cache"
      },
      "database": {
        "enabled": true,
        "table": "security_cache"
      }
    },
    "cache_managers": {
      "authentication": {
        "enabled": true,
        "ttl": 1800
      },
      "authorization": {
        "enabled": true,
        "ttl": 600
      },
      "rate_limiting": {
        "enabled": true,
        "ttl": 60
      },
      "csrf_protection": {
        "enabled": true,
        "ttl": 3600
      },
      "input_validation": {
        "enabled": true,
        "ttl": 300
      },
      "threat_detection": {
        "enabled": true,
        "ttl": 1800
      },
      "security_headers": {
        "enabled": true,
        "ttl": 86400
      },
      "compliance_checks": {
        "enabled": true,
        "ttl": 3600
      }
    },
    "performance": {
      "max_cache_size": 10000,
      "eviction_policy": "LRU",
      "compression_enabled": true,
      "serialization_format": "pickle"
    }
  }
}