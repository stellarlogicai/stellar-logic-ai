{
  "name": "helm-ai",
  "version": "1.0.0",
  "description": "Helm AI - Multi-modal intelligence with constitutional safety framework",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:core": "jest tests/helm-ai.test.js",
    "test:integration": "jest --testNamePattern=\"Integration\"",
    "test:security": "jest --testNamePattern=\"Security\"",
    "test:performance": "jest --testNamePattern=\"Performance\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "docs": "jsdoc -c jsdoc.json",
    "docs:serve": "http-server docs -p 8080",
    "dev:debug": "node --inspect server.js",
    "dev:watch": "nodemon --watch src --watch tests server.js",
    "build": "npm run lint && npm run test",
    "start:prod": "NODE_ENV=production node server.js",
    "health": "curl -s http://localhost:3001/api/health",
    "setup": "npm install && npm run build",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "ai",
    "multi-modal",
    "constitutional-ai",
    "safety",
    "security",
    "machine-learning",
    "neural-networks",
    "enterprise"
  ],
  "author": "Helm AI Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "googleapis": "^171.2.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^8.0.0",
    "socket.io": "^4.8.3",
    "sqlite3": "^5.0.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/helm-ai/helm-ai.git"
  },
  "bugs": {
    "url": "https://github.com/helm-ai/helm-ai/issues"
  },
  "homepage": "https://helm-ai.com"
}
